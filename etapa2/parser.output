Grammar

    0 $accept: programa $end

    1 programa: lista_de_elementos
    2         | /* empty */

    3 lista_de_elementos: declaracao

    4 declaracao: tipo lista_identificador

    5 lista_identificador: TK_IDENTIFICADOR array "," lista_identificador ';'
    6                    | TK_IDENTIFICADOR array
    7                    | /* empty */

    8 array: '[' TK_LIT_INT ']'
    9      | '[' lista_de_literais_inteiros ']'
   10      | /* empty */

   11 lista_de_literais_inteiros: TK_LIT_INT '^' lista_de_literais_inteiros
   12                           | /* empty */

   13 tipo: TK_PR_FLOAT
   14     | TK_PR_INT
   15     | TK_PR_CHAR
   16     | TK_PR_BOOL


